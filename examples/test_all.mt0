"Loading & Running all examples"
line_sep = "******************************"
this = "test_all.mt0"
root = File.new$("examples")
<%("Absolute path: " :: root.absolute_path$())
<%("Exists: " :: !root.exists$())
if (!root.exists$() || !root.is_dir$()) ? -> {
	die$("Failed to open examples directory")
}

tmp = File.new$("")
root.for_each(func @(file) {
	#tmp.set$(file)
	if (#tmp.extension$() != ".mt0" || file.contains$(#this)) ? -> {
		break!
	}
	newl :: file :: line_sep :: newl
	/*
	load$(file)$()
	line_sep
	*/
})