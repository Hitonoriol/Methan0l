x = 1
bool = defval: Boolean
str = defval: String

print = -> {
	<% "x = " :: x
	<% "bool = " :: bool
	<% "str = \"" :: str :: "\""
}

print()

/* Type-keeping assignment -- converts RHS to LHS' type */
x := 5.678
bool := 1
str := 0x1000
print()

/* Will throw InvalidTypeException if evaluated */
/* *[str] typesafe: str = x */